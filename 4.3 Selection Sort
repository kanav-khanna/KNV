//What if i select the minimum element at put it at the right position ?

TC-O(n square)
sc- O(1)

void selectionsort(vector <int> &v)
    {
        int n = v.size();

        for(int i = i;i<n;i++)
            {
                if(v[j]< v[minindex])
                {
                    int minindex = i;
                }
            }
        swap(v[i],v[minindex]); 
    }

///copied code 
void selectionSort(vector<int> &v) {
  int n = v.size();
  for (int i = 0; i < n - 1; i++) {
    int minIndex = i; // ith element hi smallest hai
    for (int j = i + 1; j < n; j++) {
      if (v[j] < v[minIndex]) {
        minIndex = j;
      }
    }

    // swap ith and minIndex elements;
    swap(v[i], v[minIndex]);
  }
}